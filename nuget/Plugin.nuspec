<?xml version="1.0"?>
<package >
  <metadata minClientVersion="2.8.1">
    <id>Plugin.FirebasePushNotification</id>
    <version>2.2.0-beta</version>
    <title>Firebase Push Notification Plugin for Xamarin</title>
    <authors>Rendy Del Rosario</authors>
    <owners>crossgeeks rdelrosario</owners>
    <licenseUrl>https://raw.githubusercontent.com/CrossGeeks/FirebasePushNotificationPlugin/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/CrossGeeks/FirebasePushNotificationPlugin</projectUrl>
    <iconUrl>https://github.com/CrossGeeks/FirebasePushNotificationPlugin/blob/master/art/icon.png?raw=true</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Receive and handle firebase push notifications across Xamarin.iOS and Xamarin.Android</description>
    <summary>Receive and handle firebase push notifications across Xamarin.iOS and Xamarin.Android</summary>
    <releaseNotes>Support for Android O and other minor fixes.</releaseNotes>
    <copyright>Copyright 2017</copyright>
    <tags>iOS,Android,firebase,push notifications,cloud messaging,pcl,xamarin,plugins,xam.pcl</tags>
    <dependencies>
        <group targetFramework="MonoAndroid80">
            <dependency id="Xamarin.Firebase.Messaging" version="[60.1142.0]" />
            <dependency id="Xamarin.Build.Download" version="0.4.7" />
        </group>
        <group targetFramework="Xamarin.iOS10">
            <dependency id="Xamarin.Firebase.iOS.CloudMessaging" version="2.0.4" />
            <dependency id="Xamarin.Build.Download" version="0.4.7" />
        </group>
    </dependencies>
  </metadata>
  <files>
        <!--NetStandard/PCL-->
        <file src="..\src\Plugin.FirebasePushNotification\bin\Release\netstandard1.0\Plugin.FirebasePushNotification.*" target="lib\netstandard1.0" />
        <!--Xamarin.Android-->
        <file src="..\src\Plugin.FirebasePushNotification.Android\bin\Release\Plugin.FirebasePushNotification.*" target="lib\MonoAndroid80" />
        <!--Xamarin.iOS-->
        <file src="..\src\Plugin.FirebasePushNotification.iOS\bin\iPhone\Release\Plugin.FirebasePushNotification.*" target="lib\Xamarin.iOS10" />
  </files>
</package>

